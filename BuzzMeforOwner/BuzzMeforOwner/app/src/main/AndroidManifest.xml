<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tharshini.buzzmeforowner">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Buzz Me"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CheckDailyReports" />
        <activity android:name=".DisplayGraphForTheDay" />
        <activity android:name=".CheckMonthlyReports" />
        <activity android:name=".DisplayGraphForTheMonth" />
        <activity android:name=".Profile" />
    </application>

</manifest>